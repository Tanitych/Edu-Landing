    <!-- @import "header.html" -->

    <div class="content">
      <div class="block__1">
        <div class="block__1__left">
          <h1 class="block__1__left__title">
            Закажи платформу и <br />
            <span>получи</span> мобильное <br />
            приложение <span>в подарок </span>
          </h1>
          <h3 class="block__1__left__subtitle">
            Оставь заявку , и получи мобильное приложение <br />
            абсолютно бесплатно
          </h3>
          <button class="modal__btn" id="myBtn">
            Заказать и получить бесплатно
          </button>

          <!-- The Modal -->
          <div id="myModal" class="modal">
            <!-- Modal content -->
            <div class="modal-content">
              <span class="close">&times;</span>

              <form>
                <h2 class="modal__title">Оставьте заявку</h2>
                <input type="text" placeholder="Ваше имя" required />
                <input type="email" placeholder="E-mail" required />
                <input
                  type="tel"
                  required
                  placeholder="Ваш номер телефона"
                  id="phone"
                />
                <button>Отправить заявку</button>
              </form>
            </div>
          </div>
        </div>
        <div class="block__1__right">
          <div class="img">
            <img src="img/block__1.jpg" alt="" />
          </div>
        </div>
      </div>
    </div>

    <div class="block__3" id="why_we">
